#include <iostream>
#include <string>
using namespace std;
int main()
{
    string a;
    int x = 0;
    cin >>a;
    int  b[5]= {'h','e','l','l','o'};
    for (int i=0;i<a.size();i++)
    {
        if (a[i]==b[x])
        {
            x++;
        }
    }
    if (x ==5)
     {
         cout <<"YES";
     }
     else
        cout <<"NO";
}
