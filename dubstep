#include <iostream>
using namespace std;

int main() {
	string s;
	int b=0;
	cin>>s;
	for(int i=0;i<s.size();i++){
	    if(s[i]=='W'&& s[i+1]=='U' && s[i+2]=='B'){
	        i+=2;
	        if(b>0){
	            cout<<" ";
	        }
	        continue;
	    }else {
	        b++;
	        cout<<s[i];
	    }
	}
	return 0;
}
