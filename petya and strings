#include <iostream>
#include<cstring>
# include <cctype>
#include <cstdio>
#include <string>

using namespace std;
int main ()
{  string z ;
 string y ;
   string a;
    string b;
    cin >> a;
    cin >>b;
    for (int i = 0; i<a.size(); i++)
    {
    z +=tolower(a[i]);
    }
     for (int i = 0; i<b.size(); i++)
    {
        y+= tolower(b[i]);
    }
    if (z.compare(y)<0)
    {
        cout <<"-1";
    }
   else if (z.compare(y)>0)
   {
       cout <<" 1";
   }
   else if (z.compare(y)==0)
   {
       cout <<"0";
   }
}

